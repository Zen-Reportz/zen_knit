{
  "name": "vscode-zen-knit",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zen-Reportz/zen_knit/pulgins/vscode-zen-knit"
  },
  "displayName": "vscode-zen-knit",
  "description": "Zen-Knit is a formal (PDF), informal (HTML) report generator for data analyst and data scientist who wants to use python. RMarkdown alternative. Zen-Knit is good for creating reports, tutorials with embedded python. RMarkdown alternative. Python Markdown Support. ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-zen-knit.build",
    "onLanguage:zen-knit-format"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-zen-knit.build",
        "title": "Knit Document"
      },
      {
        "command": "zen-knit-format",
        "title": "Format pyz"
      }
    ],
    "configuration":[
      {
        "title": "zen-knit",
        "properties": {
          "zen-knit.knitPath": {
						"type": "string",
						"default": "/home/aarya/.cache/pypoetry/virtualenvs/zen-knit-cH2izjWV-py3.8/bin/knit",
						"description": "zen-knit path"
					}
        }
      }
    ],
    "keybindings": [
			{
				"command": "vscode-zen-knit.build",
				"key": "ctrl+alt+k"
			}
		],
		"languages": [
			{
				"id": "zen-knit-format",
				"aliases": [
					"Zen Knit format"
				],
				"extensions": [
					"pyz"
				]
			}
		]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "dataclass": "^2.0.0"
  }
}
